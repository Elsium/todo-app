{
  "extends": [
    "eslint:recommended",
    "next/core-web-vitals"
  ],
  "rules": {
    "no-unused-vars": "off",
    "eqeqeq": "error",
    "indent": ["error", 4],
    "semi": ["error", "never"],
    "no-undef": "warn",
    "arrow-spacing": "error",
    "no-var": "error",
    "prefer-const": "warn",
    "no-duplicate-imports": "error"
  }
}
